{
  "name": "mcneely/collections",
  "require-dev": {
    "phpunit/phpunit": "^7.0",
    "phpstan/phpstan": "^0.10"
  },
  "authors": [
    {
      "name": "Paul McNeely",
      "email": "paul@paulmcneely.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "ramsey/uuid": "^3.7",
    "mcneely/core": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "Mcneely\\Collections\\": "src/"
    }
  },
  "scripts": {
    "test": "phpunit --bootstrap vendor/autoload.php tests/",
    "coverage": "phpunit --coverage-clover=coverage/coverage.xml --coverage-html=coverage --whitelist src/ --bootstrap vendor/autoload.php tests/",
    "static": "phpstan analyse -l max src/"
  },
  "config": {
    "bin-dir": "bin/"
  }
}
