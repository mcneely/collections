{
    "name": "mcneely/collections",
    "require-dev": {
      "phpunit/phpunit": "^7.1",
      "phpstan/phpstan": "^0.9.2"
    },
    "authors": [
        {
            "name": "Paul McNeely",
            "email": "paul@paulmcneely.com"
        }
    ],
    "require": {
      "php": "^7.1",
        "ramsey/uuid": "^3.7",
        "php-ds/php-ds": "^1.2"
    },
    "autoload": {
        "psr-4": {
          "Mcneely\\Collections\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit --bootstrap vendor/autoload.php tests/",
      "coverage": "phpunit --coverage-html=coverage --whitelist src/ --bootstrap vendor/autoload.php tests/",
      "static": "phpstan analyse -l 7 src/"
    }
}
