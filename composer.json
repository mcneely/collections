{
    "name": "mcneely/collections",
    "require-dev": {
        "phpunit/phpunit": "^7.1"
    },
    "authors": [
        {
            "name": "Paul McNeely",
            "email": "paul@paulmcneely.com"
        }
    ],
    "require": {
        "php":"~7.1",
        "ramsey/uuid": "^3.7",
